<p id="notice"><%= notice %></p>

<h1>Listing Products</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Goal</th>
      <th>Start date</th>
      <th>End date</th>
      <th>User</th>
      <th>Tags</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>


    <% @products.each do |product| %>
      <%=render 'product', product:product %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product', new_product_path %>
